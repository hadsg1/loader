if not game:IsLoaded() then
	game.Loaded:Wait()
end;

local players 		= game:GetService('Players');
local localPlayer 	= players.LocalPlayer;
if (not localPlayer) then
	players:GetPropertyChangedSignal('LocalPlayer'):Wait();
	localPlayer = players.LocalPlayer;
end;

local executor 		= identifyexecutor and identifyexecutor() or 'Unknown';

local messagebox 	= messagebox;
local request 		= request or http_request;
local loadstring 	= loadstring;

if (type(messagebox) ~= 'function') then
	return localPlayer:Kick('[ecstasy.wtf] missing alias ( messagebox ) - unsupported executor');
end;

local protectedMessagebox = function(body, title, id)
	local success, output = pcall(messagebox, body, title, id);
	if (not success) then
		localPlayer:Kick(`[ecstasy.wtf] messagebox_error - {body}`);
		task.wait(9e9);
		return;
	end;
	return output;
end;

if (type(loadstring) ~= 'function') then
	return protectedMessagebox(`missing alias ( loadstring ) - unsupported executor`, `ecstasy.wtf [{executor}]`, 48);
elseif (type(request) ~= 'function') then
	return protectedMessagebox(`missing alias ( request ) - unsupported executor`, `ecstasy.wtf [{executor}]`, 48);
elseif (not Drawing) then
	local success, response = pcall(request, {Url='https://raw.githubusercontent.com/hadsg1/extaze/refs/heads/main/main.lua'; Method='GET';});
	if (not success) then
		protectedMessagebox(`failed(1) - request error\n\nurl: https://raw.githubusercontent.com/hadsg1/extaze/refs/heads/main/main.lua`, `ecstasy.wtf [{executor}]`, 48);
		task.wait(9e9);
		return;
	elseif (type(response) ~= 'table' or type(response.Body) ~= 'string' or response.StatusCode ~= 200) then
		protectedMessagebox(`failed(2) - bad response\n\nurl: https://raw.githubusercontent.com/hadsg1/extaze/refs/heads/main/main.lua`, `ecstasy.wtf [{executor}]`, 48);
		task.wait(9e9);
		return;
	end;
	local loader = loadstring(response.Body);
	if (not loader) then
		protectedMessagebox(`failed(3) - syntax error\n\nurl: https://raw.githubusercontent.com/hadsg1/extaze/refs/heads/main/main.lua`, `ecstasy.wtf [{executor}]`, 48);
		task.wait(9e9);
		return;
	end;
	loader();
end;

local dir = 'https://raw.githubusercontent.com/hadsg1/extaze/refs/heads/main/';

local statuslist = {};

statuslist.fallensurvival = {
	name 		= 'Fallen Survival';
	status 		= 'Undetected';
	support 	= {'Wave'; 'AWP'; 'Hydrogen'; 'Seliware'; };
};

local success, response = pcall(request, {Url=`{dir}fallensurvival/main.lua`; Method='GET';});
if (not success) then
	protectedMessagebox(`failed(1) - request error\n\nurl: {dir}fallensurvival/main.lua`, `ecstasy.wtf [{executor}]`, 48);
	task.wait(9e9);
	return;
elseif (type(response) ~= 'table' or type(response.Body) ~= 'string' or response.StatusCode ~= 200) then
	protectedMessagebox(`failed(2) - bad response\n\nurl: {dir}fallensurvival/main.lua`, `ecstasy.wtf [{executor}]`, 48);
	task.wait(9e9);
	return;
end;
local loader = loadstring(response.Body);
if (not loader) then
	protectedMessagebox(`failed(3) - syntax error\n\nurl: {dir}fallensurvival/main.lua`, `ecstasy.wtf [{executor}]`, 48);
	task.wait(9e9);
	return;
end;
loader();
